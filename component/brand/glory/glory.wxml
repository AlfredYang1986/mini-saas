<!-- component/brand-glory/brand-glory -->
<view class="glory">
	<view class="content-page">
		<view class="content-item">
			<view class="content-row">
				<text class="title">荣誉奖项</text>
			</view>
      <view class="content-row" wx:for="{{awards}}">
        <view class="one-des">
          <text class="">{{item.title}}</text>
        </view>
        <view class="one-des">
          <text class="">{{item.date}}</text>
        </view>
        <view class="one-des">
          <text class="">{{item.level}}</text>
        </view>
			</view>
			<view class="content-row">
				<text class="title">赛事资格</text>
			</view>
			<view class="content-row" wx:for="{{attends}}">
        <view class="one-des">
          <text class="">{{item.title}}</text>
        </view>
			</view>
			<view class="content-row">
				<text class="title">资格认证</text>
			</view>
			<view class="content-row" wx:for="{{qualifier}}">
        <view class="one-des-more">
          <text class="">{{item.title}}</text>
        </view>
    	</view>
		</view>
	</view>
</view>
