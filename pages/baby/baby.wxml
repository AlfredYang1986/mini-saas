  <text class='title'>即将开始</text>
  <view class='image'></view>

  <acti-list  bindtap='showRecord'></acti-list>

  <cla-list wx:if="{{falsee}}" isOver="{{falsee.value}}" bindtap="showRecord"></cla-list>
  
  <view class='over'>
    <view class='over-title'>已结束</view>
    <view class='border'></view>
  </view>

  <cla-list wx:if="{{truee}}" isOver="{{truee.value}}" bindtap="showRecord"></cla-list>

</view>